<?xml version="1.0" encoding="utf-8" ?>
<demisauce len="{{c.len}">
% for item in c.cmsitems:
    <cmsitem id="{{item.id}" key="{{item.key}" rid="{{item.rid}">
        <title>{{item.title}</title>
        <content><![CDATA[{{item.content}]]></content>
        <content2><![CDATA[{{item.content2}]]></content2>
        <created>{{item.created}</created>
        <resourceid>{{item.rid}</resourceid>
        <itemtype>{{item.item_type}</itemtype>
        <url>{{item.url}</url>
    </cmsitem>
{% endfor %}
</demisauce>
